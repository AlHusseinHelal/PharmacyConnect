                  </label>
                </div>
                <div class="radio-button">
                  <input name="answerc2" id="radio6" class="radio-button__input" type="radio"
                    value="<%= exam.answer2questionchoose2 %>">
                  <label for="radio6" class="radio-button__label">
                    <span class="radio-button__custom"></span>

                    <%= exam.answer2questionchoose2 %>
                  </label>
                </div>
                <div class="radio-button">
                  <input name="answerc2" id="radio7" class="radio-button__input" type="radio"
                    value="<%= exam.answer3questionchoose2 %>">
                  <label for="radio7" class="radio-button__label">
                    <span class="radio-button__custom"></span>

