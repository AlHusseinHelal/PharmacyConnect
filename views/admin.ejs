          <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z" />
          <path fill-rule="evenodd"
            d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8zm8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1z" />
        </symbol>
      </svg>



      <main class="container m-0" style="width: 100%; max-width: unset; height: 90%; ">
        <div class="d-flex flex-column flex-md-row h-100 p-3 row justify-content-md-around ">

          <div class="mh-100 border col-md-5 rounded-3 p-2 overflow-y-auto"
            style="box-shadow: 0px 0px 4px 3px rgba(178, 178, 178, 0.6);">
            <div class="container2">

              <div class="accordion">

                <div class="accordion-item">
                  <button class="but" id="accordion-button-1" aria-expanded="false"><span
                      class="accordion-title">Users</span><span class="icon" aria-hidden="true"></span></button>
                  <% if (array.length>= 1) { %>
                    <div class="accordion-content">
                      <% array.forEach(item=> { %>

                        <div class="d-flex align-items-center justify-content-between mb-2 mt-5">
                          <div class="d-flex align-items-center">
                            <img class="me-2" style="border-radius: 50%;" width="20" height="20"
                              src="<%= item.profileimage %>" alt=""><span>
                              <%= item.firstname + "." + item.lastname %>
                            </span><span>-<%= item.code %></span>
                          </div>



                          <div class="d-flex align-items-center">
                            <div class="d-flex align-items-center me-2">
                              
                              <a data-tooltip="DIC" data-flow="top" class=" <%= item.role === "DIC"? "d-block"
                                : "d-none" %>" href=""><i class="fa-solid fa-d"></i></a>
                                <a data-tooltip="Nurse" data-flow="top" class=" <%= item.role === "Nurse"? "d-block"
                                : "d-none" %>" href=""><i class="fa-solid fa-n"></i></a>
                              <a data-tooltip="Admin" data-flow="top" class=" <%= item.role === "Admin"? "d-block"
                                : "d-none" %>" href=""><i class="fa-solid fa-screwdriver-wrench"
                                  style="color: #df0707;"></i></a>
                              <form action="/lockuser/<%= item._id %>?_method=PUT" method="post"><button style="border: unset; color: unset; background-color: unset;" class=" <%= item.active === true? "" : "d-none" %>" href="" data-tooltip="Active" data-flow="top"><i class="ms-2 fa-regular fa-unlock" style="color: #74C0FC;"></i></button></form>
                              <form action="/unlockuser/<%= item._id %>?_method=PUT" method="post"><button style="border: unset; background-color: unset;" class="ms-2 <%= item.active === false? "" : "d-none" %>" data-tooltip="Locked" data-flow="top"><i
                                class="fa-regular fa-lock" style="color: red;"></i></button></form>
                            </div>
                            <div class="d-flex align-items-center">
                              <a href="" data-bs-toggle="modal" data-bs-target="#exampleModal<%= item._id %>"
                                data-tooltip="Role" data-flow="top"><i class="fa-solid fa-user-tie"
                                  style="color: #74c0fc;"></i></a>
                              <a class="ms-2 me-1" data-bs-toggle="modal"
                                data-bs-target="#exampleModal2<%= item._id %>"><i class="fa-solid fa-trash-can"
                                  style="color: #df0707;"></i></a>
                            </div>
                          </div>

                        </div>
                        <li class="border-top notlist" style="list-style: none;"></li>

                        <!-- MODAL DELETE USER -->
                        <div class="connectmodal modal fade" id="exampleModal2<%= item._id %>" tabindex="-1"
                          aria-labelledby="exampleModalLabel" aria-hidden="true">
                          <div class="glass modal-dialog">
                            <div class="modal-content">
                              <div class="modal-header">
                                <h1 class="modal-title fs-5" id="exampleModalLabel">Delete User</h1>
                                <button type="button" class="btn-close" data-bs-dismiss="modal"
                                  aria-label="Close"></button>
                              </div>
                              <div class="modal-body">
                                Are You Sure ?
                              </div>
                              <div class="modal-footer">
                                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                <form action="/deleteuser/<%= item._id %>?_method=DELETE" method="post">
                                  <button type="submit" class="btn btn-danger">Delete</button>
                                </form>
                              </div>
                            </div>
                          </div>
                        </div>

                        <!-- MODAL CHANGE USER ROLE -->
                        <div class="connectmodal modal fade" id="exampleModal<%= item._id %>" tabindex="-1"
                          aria-labelledby="exampleModalLabel" aria-hidden="true">
                          <div class="glass modal-dialog">
                            <div class="modal-content">
                              <div class="modal-header">
                                <h1 class="modal-title fs-5" id="exampleModalLabel">Select User Role</h1>
                                <button type="button" class="btn-close" data-bs-dismiss="modal"
                                  aria-label="Close"></button>
                              </div>
                              <form action="/userole/<%= item._id %>?_method=PUT" method="post">
                                <div class="modal-body">
                                  <select class="form-control w-100" name="role">
                                    <option hidden disabled selected value="User">Choose...</option>
                                    <option value="User">User</option>
                                    <option value="DIC">DIC</option>
                                    <option value="Admin">Admin</option>
                                    <option value="Nurse">Nurse</option>
                                  </select>
                                </div>
                                <div class="modal-footer">
                                  <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                  <button type="submit" class="btn btn-primary">Save changes</button>
                              </form>
                            </div>
                          </div>
                        </div>
                    </div>



                    <% }) %>
                </div>
                <% } %>
              </div>




