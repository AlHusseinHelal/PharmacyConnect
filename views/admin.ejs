                    <span class="radio-button__custom"></span>

                    <%= exam.answer3questionchoose4 %>
                  </label>
                </div>
                <div class="radio-button">
                  <input name="answerc4" id="radio16" class="radio-button__input" type="radio"
                    value="<%= exam.answer4questionchoose4 %>">
                  <label for="radio16" class="radio-button__label">
                    <span class="radio-button__custom"></span>

                    <%= exam.answer4questionchoose4 %>
                  </label>
                </div>
              </div>

