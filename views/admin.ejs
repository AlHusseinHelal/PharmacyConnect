                  </div>
                  <div class="w-100 d-flex my-2 justify-content-center">
                    <select name="" class="form-control" type="text" id="selectAssy" style="width: 98%;">
                      <option hidden disabled selected value="">Question#</option>
                      <option value="1">1</option>
                      <option value="2">2</option>
                      <option value="3">3</option>
                      <option value="4">4</option>
                      <option value="5">5</option>
                      <option value="6">6</option>
                      <option value="7">7</option>
                      <option value="8">8</option>
                      <option value="9">9</option>
                      <option value="10">10</option>
                    </select>
                  </div>

                  <div class="d-flex align-items-center ms-1 mt-2 ">
                    <i class="fa-solid fa-list me-2" style="color: #74C0FC;"></i>
                    <span class=""
                      style="font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif">Complete</span>
                  </div>
                  <div class="w-100 d-flex my-2 justify-content-center">
                    <select name="" class="form-control" type="text" id="selectComplete" style="width: 98%;">
                      <option hidden disabled selected value="">Question#</option>
                      <option value="1">1</option>
                      <option value="2">2</option>
                      <option value="3">3</option>
                      <option value="4">4</option>
                      <option value="5">5</option>
                      <option value="6">6</option>
                      <option value="7">7</option>
                      <option value="8">8</option>
                      <option value="9">9</option>
                      <option value="10">10</option>
                    </select>
                  </div>

                  <div class="d-flex align-items-center ms-1 mt-2 ">
                    <i class="fa-solid fa-check me-2" style="color: #74C0FC;"></i>
                    <span class=""
                      style="font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif">True/False</span>
                  </div>
                  <div class="w-100 d-flex my-2 justify-content-center">
                    <select name="" class="form-control" type="text" id="selectTrue" style="width: 98%;">
                      <option hidden disabled selected value="">Question#</option>
                      <option value="1">1</option>
                      <option value="2">2</option>
                      <option value="3">3</option>
                      <option value="4">4</option>
                      <option value="5">5</option>
                      <option value="6">6</option>
                      <option value="7">7</option>
                      <option value="8">8</option>
                      <option value="9">9</option>
                      <option value="10">10</option>
                    </select>
                  </div>


                  <div class=" d-flex justify-content-end" style="width: 98%;">
                    <button class="btn btn-primary" id="submitChoose">Submit</button>
                  </div>


                  <div id="line" class="w-100 my-4 d-none" style="border-top: 2px solid #03b5d2;"></div>


                  <form action="/examform" method="post" class="d-flex flex-column align-items-center">
                    <div id="chooseForm" class="d-flex flex-column align-items-center w-100">

                    </div>
                    <input id="questionnumber" hidden type="text" value="" name="questionnumber">
                    <div id="submitbuttonchooseform" class="justify-content-end d-none" style="width: 98%;">
                      <button class="btn btn-success" type="submit">Submit</button>
                    </div>

                  </form>
                </div>
              </div>

              <div class="d-flex mt-2">
                <button id="review" class="btn btn-primary me-2">Review</button>
                <button id="results" class="btn btn-success">Results</button>
                <!-- <button id="search" class="btn btn-danger ms-2">Search</button> -->
              </div>
            </div>
          </div>



        </div>




        <div class="mh-100 border col-md-6 rounded-3 p-2 overflow-y-auto"
          style="box-shadow: 0px 0px 4px 3px rgba(178, 178, 178, 0.6);">



          <div id="exam" class="d-none">
            <div class="text-center <%= exam.examname !== undefined? " d-block" : "d-none" %> mt-3">
              <p class="city">
                <%= exam.examname %>
              </p>
            </div>
            <!-- CHOOSE -->
            <!-- QUSETION NUMBER 1 -->
            <div class="info w-100 d-flex flex-column align-items-start mb-2 <%= exam.questionchoose1 === undefined? "d-none" : "d-block" %>">
              <div class="info__title mb-3 d-flex w-100 justify-content-between">
                <div>Choose Q#1</div>

                <div class="d-flex ">
                  <div> <img class="<%= exam.answerc1 === exam.correctanswerquestion1? " d-block" : "d-none" %>"
                    src="/svg/true-06.svg" width="25" height="25" alt=""></div>
                  <div> <img class="<%= exam.answerc1 !== exam.correctanswerquestion1 && exam.answerc1 !== undefined? "d-block" : "d-none" %>" src="/svg/false-05.svg" width="25" height="25" alt=""></div>
                </div>
              </div>
              <div class="info__title mb-3">
                <%= exam.questionchoose1 %>

              </div>

              <div class="radio-buttons-container w-100 d-flex justify-content-around">
                <div class="radio-button">
                  <input name="answerc1" id="radio2" class="radio-button__input" type="radio"
                    value="<%= exam.answer1questionchoose1 %>">
                  <label for="radio2" class="radio-button__label">
                    <span class="radio-button__custom"></span>

                    <%= exam.answer1questionchoose1 %>
                  </label>
                </div>
                <div class="radio-button">
                  <input name="answerc1" id="radio1" class="radio-button__input" type="radio"
                    value="<%= exam.answer2questionchoose1 %>">
                  <label for="radio1" class="radio-button__label">
                    <span class="radio-button__custom"></span>

                    <%= exam.answer2questionchoose1 %>
                  </label>
                </div>
                <div class="radio-button">
                  <input name="answerc1" id="radio3" class="radio-button__input" type="radio"
                    value="<%= exam.answer3questionchoose1 %>">
                  <label for="radio3" class="radio-button__label">
                    <span class="radio-button__custom"></span>

                    <%= exam.answer3questionchoose1 %>
                  </label>
                </div>
                <div class="radio-button">
                  <input name="answerc1" id="radio4" class="radio-button__input" type="radio"
