                  </label>
                </div>
                <div class="radio-button">
                  <input name="answerc2" id="radio8" class="radio-button__input" type="radio"
                    value="<%= exam.answer4questionchoose2 %>">
                  <label for="radio8" class="radio-button__label">
                    <span class="radio-button__custom"></span>

                    <%= exam.answer4questionchoose2 %>
                  </label>
                </div>
              </div>

            </div>

            <!-- QUSETION NUMBER 3 -->
