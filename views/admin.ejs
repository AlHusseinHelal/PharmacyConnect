                      <span class="my-2"
                        style="font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif">Presentation Title</span>
                    </div>
                    <div class="my-2">
                      
                        <form action="/deletepresentation?_method=DELETE" method="post">
                          <div class="d-flex" style="width: 98%;">
                          <input  type="text" class="form-control" name="title" id="title">
                          <button  type="submit" class="btn btn-danger ms-1">Delete</button>
                          </div>
                        </form>
                      
                      

