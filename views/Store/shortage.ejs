                      <th class="text-center" scope="col">Category Name</th>
                      <th class="text-center" scope="col">Org Number</th>
                      <th class="text-center" scope="col">On Hand Quantity</th>
                      <th class="text-center" scope="col">Stock Months</th>
                      <th class="text-center <%= user.role === "Admin" || user.role === "Pyxis"? "" : "d-none" %>" scope="col">ADD</th>
                      <th class="text-center <%= user.role === "Admin" || user.role === "Pyxis"? "" : "d-none" %>" scope="col">REMOVE</th>
                    </tr>
                  </thead>
  
  
