                    <tr id="tr">
                      <style>
                        .bgyellow{
                          color: rgb(255, 0, 0);
                        }
                      </style>
                      <td class="text-center bgyellow">
                        <strong class="<%= item.tradeselect === "select"? "bgyellow" : "" %>" ><%= item.CATEGORYNAME %></strong>
                      </td>
                      <td class="text-center">
                        <%= item.OrgName %>
                      </td>
                      <td class="text-center">
