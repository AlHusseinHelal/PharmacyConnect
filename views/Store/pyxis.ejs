      <main class="d-flex flex-nowrap">
        <!-- /*--------------------------------------------------------------
    # Arrow Sidebar
    --------------------------------------------------------------*/ -->
        <%- include("../partials/arrow sidebar.ejs") %>

          <div style="position: relative; height: 90vh" class="w-100">
            <!-- /*--------------------------------------------------------------
      # NAVBAR
      --------------------------------------------------------------*/ -->
            <div>
              <nav class="navbar navbar-expand-lg bg-body-tertiary mx-1 mt-1 mb-5">
                <div class="container-fluid py-2 rounded-2 bg-info">
                  <div class="d-flex flex-row align-items-center">
                    <img src="/svg/ConnectLogo.svg" height="35vh" alt="" />
                  </div>

                  <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                  </button>
                  <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <form class="d-flex" action="/pyxissearch" method="post">
                      <input name="searchText" class="form-control me-2" type="search" placeholder="Search" aria-label="Search" />
                      <button class="btn btn-outline-success" type="submit">
                        Search
                      </button>
                    </form>
                  </div>
                </div>
              </nav>
            </div>

            <!-- /*--------------------------------------------------------------
      # MAIN CONTENT
      --------------------------------------------------------------*/ -->
            <div class="d-block" style="width: 95%; margin-left: auto; margin-right: auto">
              <% if (array.length === 0) { %>
                <div class="d-flex justify-content-center">
                  <div class="alert alert-success new2 p-4 w-50 mt-5">
                    <i class="fa fa-info-circle" aria-hidden="true"></i>
                    <span class="alert-body">
                      <h6 class="alert-header">ATTENTION</h6>
                      <p class="mb-0">No Data Avaliable</p>
                    </span>
                  </div>
                </div>
              <% } %>
              
                <table style="width: 99%" class="mx-auto table table-striped table-bordered">
                  <% if (array.length >= 1) { %>
                   
                
                  <thead>
                    <tr>
                      <th class="text-center" scope="col">Org Code</th>
                      <th class="text-center" scope="col">Item Number</th>
                      <th class="text-center" scope="col">Item Description</th>
                      <th class="text-center" scope="col">UOM</th>
                      <th class="text-center" scope="col">Item Category</th>
                      <th class="text-center" scope="col">Medications Stock</th>
                    </tr>
                  </thead>
  
  
                <% array.forEach(item => { %>
                  <tbody id="tbody">
                    <tr id="tr">
                      <td class="text-center">
                        <%= item.OrgCode %>
                      </td>
                      <td class="text-center">
                        <%= item.ItemNumber %>
                      </td>
                      <td class="text-center">
                        <%= item.ItemDescription %>
                      </td>
                      <td class="text-center">
                        <%= item.UOM %>
                      </td>
                      <td class="text-center">
                        <%= item.ItemCategory %>
                      </td>
                      <td class="text-center">
                        <%= item.MedicationsStock %>
                      </td>
                    </tr>
                  </tbody>
                <% }) %>
                <% } %>
              
                  
                  
              </table>
            </div>
          </div>
      </main>

      <!-- LINK TO jQuary -->
      <script src="/js/jquery-3.7.1.min.js"></script>

