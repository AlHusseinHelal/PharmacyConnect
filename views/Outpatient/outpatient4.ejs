      <main class="d-flex flex-nowrap">
        <!-- /*--------------------------------------------------------------
    # Arrow Sidebar
    --------------------------------------------------------------*/ -->
        <%- include("../partials/arrow sidebar.ejs") %>

          <!-- /*--------------------------------------------------------------
    # SIDE LIST
    --------------------------------------------------------------*/ -->
          <div class="d-flex flex-column flex-shrink-0 p-3 bg-body-tertiary sidelist">
            <img id="connectlogo" src="/svg/PharmacyConnectLogooutpatient-01.svg" height="139vh" alt="" />

            <hr />
            <ul class="nav navul nav-pills flex-column mb-auto mainul">
              <li>
                <div class="d-inline-flex align-items-center">
                  <a href="/outpatient2" style="color: black"
                    class="nav-link d-inline-flex align-items-center width side-bg">
                    <svg class="margin side-svg" style="width: 30px; height: 30px">
                      <use xlink:href="#people-circle" />
                    </svg>
                    <span class="side-text">Add Patient</span>
                  </a>
                </div>
              </li>
              <li>
                <div class="d-inline-flex align-items-center" style="width: 164.56px">
                  <a href="/outpatient3" class="nav-link active d-inline-flex align-items-center width side-bg">
                    <svg class="margin side-svg" style="width: 30px; height: 30px; fill: white">
                      <use xlink:href="#table" />
                    </svg>
                    <span class="side-text">OverView</span>
                  </a>
                </div>
              </li>
            </ul>
            <hr />
            <div class="dropdown">
              <a href="#" class="d-flex align-items-center link-body-emphasis text-decoration-none dropdown-toggle"
                data-bs-toggle="dropdown" aria-expanded="false">
                <img src="https://github.com/mdo.png" alt="" width="32" height="32" class="rounded-circle me-2" />
                <strong>mdo</strong>
              </a>
              <ul class="dropdown-menu text-small shadow">
                <li><a class="dropdown-item" href="#">New project...</a></li>
                <li><a class="dropdown-item" href="#">Settings</a></li>
                <li><a class="dropdown-item" href="#">Profile</a></li>
                <li>
                  <hr class="dropdown-divider" />
                </li>
                <li><a class="dropdown-item" href="#">Sign out</a></li>
              </ul>
            </div>
          </div>

          <!-- /*--------------------------------------------------------------
    # DEVIDER
    --------------------------------------------------------------*/ -->

          <div style="position: relative; height: 90vh" class="w-100">
            <!-- /*--------------------------------------------------------------
      # NAVBAR
      --------------------------------------------------------------*/ -->
            <div>
              <nav class="navbar navbar-expand-lg bg-body-tertiary mx-1 mt-1 mb-5">
                <div class="container-fluid py-2 rounded-2 bg-info">
                  <div class="d-flex flex-row align-items-center">
                    <img src="/svg/ConnectLogo.svg" height="35vh" alt="" />
                  </div>

                  <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                  </button>
                  <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <form class="d-flex" role="search">
                      <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search" />
                      <button class="btn btn-outline-success" type="submit">
                        Search
                      </button>
                    </form>
                  </div>
                </div>
              </nav>
            </div>

            <!-- /*--------------------------------------------------------------
      # MAIN CONTENT
      --------------------------------------------------------------*/ -->

            <div id="maincontent">
              <table style="width: 80%" class="ms-3 table table-striped table-bordered">
              
                <tbody id="tbody">
                  <tr id="tr">
                    <th class="text-center">Patient Name</th>
                    <td class="text-center">
                      <%= obj.patientname %>
                    </td>
                  </tr>
                  <tr id="tr">
                    <th class="text-center">MRN</th>
                    <td class="text-center">
                      <%= obj.mrn %>
                    </td>
                  </tr>
                  <tr id="tr">
                    <th class="text-center">Patient Floor</th>
                    <td class="text-center">
                      <%= obj.ptfloor %>
                    </td>
                  </tr>
                  <tr id="tr">
                    <th class="text-center">Request Type</th>
                    <td class="text-center">
                      <%= obj.requestype %>
                    </td>
                  </tr>
                  <tr id="tr">
                    <th class="text-center">Oral/IV</th>
                    <td class="text-center">
                      <%= obj.oraliv%>
                    </td>
                  </tr>
                  <tr id="tr">
                    <th class="text-center">Round Comment</th>
                    <td class="text-center">
                      <%= obj.roundcomment%>
                    </td>
                  </tr>
                  <tr id="tr">
                    <th class="text-center">Round Time</th>
                    <td class="text-center">
                      <%= moment(obj.createdAt).format('LT')%>
                    </td>
                  </tr>
                  <tr id="tr">
                    <th class="text-center">Prep Comment</th>
                    <td class="text-center">
                      <%= obj.prepcomment %>
                    </td>
                  </tr>
                  <tr id="tr">
                    <th class="text-center">Prep Time</th>
                    <td class="text-center">
                      <%= moment(obj.updatedAt).format('LT')%>
                    </td>
                  </tr>
                </tbody>
              
              </table>

                <!-- Button trigger modal -->
                <button type="button" class="btn btn-danger  ms-3" data-bs-toggle="modal" data-bs-target="#exampleModal">
                  Delete
                </button>
  
                <!-- Modal -->
                <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel"
                  aria-hidden="true">
                  <div class="modal-dialog">
                    <div class="modal-content">
                      <div class="modal-header">
                        <h1 class="modal-title fs-5" id="exampleModalLabel">Warning</h1>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                      </div>
                      <div class="modal-body">
                       Are you sure you want to delete ?
                      </div>
                      <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                        <form action="/deleteout/<%= obj._id%>?_method=DELETE" method="post">
                        <button type="submit" class="btn btn-danger">Delete</button>
                      </form>
                      </div>
                    </div>
                  </div>
                </div>
            </div>
          </div>
