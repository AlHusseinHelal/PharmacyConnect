        <!-- SEARCH Medication -->
        <script>
          searchmedication.addEventListener("click", async (event) => {
            event.preventDefault()
            const res = await fetch("/searchmedication", {
              method: "POST",
              body: JSON.stringify({ searchinput: searchinput.value }),
              headers: { "Content-Type": "application/json" },
            })
            const data = await res.json()
            if (data.errors) {
              data.errors.forEach(item => {
                if (item.path = "searchinput") {
                  nosearchquestion.classList.remove("d-none");
                  searchinput.classList.add("redframe");
                } else {
                  nosearchquestion.classList.add("d-none");
                  searchinput.classList.remove("redframe");
                }
              });
            }
            if (data.nodata) {
              nodata.classList.remove("d-none");
            } else {
              nodata.classList.add("d-none");
                }

          if (data.done) {
            const select = searchinput.value
            const medication = document.querySelectorAll(".medication")
            console.log(medication);  
            const array = []
            medication.forEach( item => {
              const generic = item.querySelector(".generic").textContent
              const code = item.querySelector(".code").textContent
              array.push({generic,code})
            
              
            })
            console.log(array);
            const match = array.find( item => item.generic.match(select.toUpperCase()))
            const code = match.code
            const med = document.querySelector("." + code).classList.add("back");
          
            document.querySelector("." + code).parentElement.parentElement.classList.add("show")
            document.querySelector("." + code).parentElement.parentElement.parentElement.parentElement.parentElement.classList.add("show");
            document.querySelector("." + code).parentElement.parentElement.parentElement.firstElementChild.previousElementSibling;
            document.querySelector("." + code).parentElement.parentElement.parentElement.setAttribute("aria-expanded", "true");  
          }
          
          })
        </script>
        


